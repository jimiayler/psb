<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.metaharmonic.igottabreathe"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>"I Gotta Breathe": A Post-Sigularity Blues</name>

    <description>
        A multimedia holler for the less-than-pre-human.
    </description>

    <author href="http://www.igottabreathe.com" email="comments@igottabreathe.com">
        James Keepnews 
    </author>

    <gap:platform name="ios" />
    <gap:platform name="android" />

    <preference name="orientation" value="default" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="exit-on-suspend" value="true" />

    <icon src="icon.png" />

    <icon src="img/icon-57.png" gap:platform="ios" width="57" height="57" />
    <icon src="img/icon-72.png" gap:platform="ios" width="72" height="72" />
    <icon src="img/icon_at_2x.png" gap:platform="ios" width="114" height="114" />

    <!-- retina iPad support: PhoneGap 2.5.0+ only -->
    <icon src="img/icon-72_at_2x.png" gap:platform="ios" width="144" height="144" />

    <icon src="img/icon-57.png" gap:platform="android" gap:density="ldpi" />
    <icon src="img/icon-72.png" gap:platform="android" gap:density="mdpi" />
    <icon src="img/icon_at_2x.png" gap:platform="android" gap:density="hdpi" />
    <icon src="img/icon-72_at_2x.png" gap:platform="android" gap:density="xhdpi" />

    

</widget>